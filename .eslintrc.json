{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",

    "react/prop-types": "off",

    "no-unused-vars":"warn",
    "no-console": "warn",
    "no-shadow": "off",
    "react/state-in-constructor":"off",
    "react/destructuring-assignment":"off"
  }
}
